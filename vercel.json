{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "functions": {
    "src/app/api/**": {
      "maxDuration": 30,
      "excludeFiles": "{node_modules/**,public/**,.next/cache/**,docs/**,tests/**,android/**,ios/**,uploads/**,tmp/**}"
    },
    "src/app/**": {
      "excludeFiles": "{node_modules/**,public/**,.next/cache/**,docs/**,tests/**,android/**,ios/**,uploads/**,tmp/**}"
    }
  },
  "regions": ["fra1"]
}